{
"manifest_version": 2,
    "name": "Replace \"mask\" with \"pants\"",
    "description": "Replaces all instances of the word \"mask\" with the word \"pants\".",
    "version": "1.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
